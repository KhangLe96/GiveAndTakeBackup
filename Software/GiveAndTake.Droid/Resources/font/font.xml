<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto">

	<!-- regular -->
	<font
		android:font="@font/sanfranciscodisplay_regular"
		android:fontStyle="normal"
		android:fontWeight="400"
		app:font="@font/sanfranciscodisplay_regular"
		app:fontStyle="normal"
		app:fontWeight="400"/>

	<!-- bold -->
	<font
		android:font="@font/sanfranciscodisplay_bold"
		android:fontStyle="normal"
		android:fontWeight="700"		
		app:font="@font/sanfranciscodisplay_bold"
		app:fontStyle="normal"
		app:fontWeight="700"/>
	
	<!-- light -->
	<font
		android:font="@font/sanfranciscodisplay_light"
		android:fontStyle="normal"
		android:fontWeight="300"	
		app:font="@font/sanfranciscodisplay_light"
		app:fontStyle="normal"
		app:fontWeight="300"/>
		
	<!-- medium -->
	<font
		android:font="@font/sanfranciscodisplay_medium"
		android:fontStyle="normal"
		android:fontWeight="500"
		app:font="@font/sanfranciscodisplay_medium"
		app:fontStyle="normal"
		app:fontWeight="500"/>

</font-family>